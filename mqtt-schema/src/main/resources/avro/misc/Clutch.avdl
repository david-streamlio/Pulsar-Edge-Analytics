@namespace("com.manning.pulsar.iiot.clutch")

protocol ClutchProtocol {

  record ClutchSensor {
  	int sensor_id;
  	double bearing_1_temp; 
  	double bearing_2_temp;
  	double bearing_3_temp;
  	double bearing_4_temp;
  	double bearing_5_temp;
  	double bearing_6_temp; 
  	double thrust_bearing_temp;
  	double clutch_attitude;
  }
  
}